{
    "defaultSeverity": "warn",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "class-name":true,
        "comment-format":[true, "check-space"],
        "encoding":true,
        "forin":false,
        "max-line-length":[true, 128],
        "no-var-keyword":false,
        "no-boolean-literal-compare":true,
        "no-trailing-whitespace":true,
        "prefer-const": false,
        "prefer-for-of":false,
        "semicolon":true
    },
    "rulesDirectory": ["tslint-plugin-prettier"]
}
